{
  "name": "diffusers.js",
  "version": "0.9.3",
  "license": "MIT",
  "author": {
    "name": "Rana",
    "url": "https://rana.ai"
  },
  "type": "module",
  "dependencies": {
    "onnxruntime-web": "1.19.2",
    "@huggingface/hub": "^0.8.7",
    "@techstark/opencv-js": "^4.10.0-release.1",
    "@types/node": "^16.18.109",
    "@xenova/transformers": "^2.17.2",
    "cli-progress": "^3.12.0",
    "idb": "^7.1.1",
    "module-alias": "^2.2.3",
    "onnxruntime-common": "1.16.1",
    "onnxruntime-node": "1.16.1",
    "seedrandom": "^3.0.5",
    "sharp": "^0.32.6",
    "ts-whammy": "^1.1.5",
    "wasm-feature-detect": "^1.8.0"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:ts && npm run build:types",
    "build:ts": "node scripts/build.js",
    "build:types": "tsc --emitDeclarationOnly",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "lint": "eslint --ext .ts,.tsx src"
  },
  "main": "dist/index.js",
  "exports": {
    ".": {
      "node": {
        "require": "./dist/index-node.cjs",
        "import": "./dist/index-node.esm.js"
      },
      "default": {
        "require": "./dist/index.js",
        "import": "./dist/index.esm.js"
      }
    }
  },
  "engines": {
    "node": ">=18"
  },
  "browser": {
    "fs": false,
    "path": false,
    "url": false,
    "sharp": false,
    "util": false,
    "onnxruntime-node": false,
    "cli-progress": false,
    "stream/web": false
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.7",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.9",
    "@types/seedrandom": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "clang-format": "^1.8.0",
    "esbuild": "0.19.4",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2"
  },
  "overrides": {
    "@xenova/transformers": {
      "onnxruntime-common": "1.16.1",
      "onnxruntime-node": "1.16.1"
    }
  },
  "_moduleAliases": {
    "@": "src"
  }
}
